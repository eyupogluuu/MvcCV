@model MvcCV.Models.Entity.experince
@{
    ViewBag.Title = "experinceUpdate";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}

<br />
<h2 class="card-header" style="color: white; background-color: gray; text-align: center ">Deneyim GÜncelleme Sayfası</h2>
<br />

<div style="background-color:darkgray; border:outset; color:white ">
    <form method="post">
        <br />
        @Html.Label("Deneyim ID")<br />
        @Html.TextBoxFor(x => x.expID, new { @class = "form-control" })
        <br />
        @Html.Label("Şirket")<br />
        @Html.TextBoxFor(x => x.company, new { @class = "form-control" })
        <br />
        @Html.Label("Ünvan")<br />
        @Html.TextBoxFor(x => x.tittle, new { @class = "form-control" })
        @Html.Label("Görev")<br />
        @Html.TextBoxFor(x => x.subtittle, new { @class = "form-control" })
        <br />
        @Html.Label("Tarih")<br />
        @Html.TextBoxFor(x => x.date, new { @class = "form-control" })<br />
        <button style="border-radius:12px;" class="btn-outline-dark"> Kaydet </button>
        <br /> <br />
    </form>
</div>

